{
  "name": "@rnbridge/web",
  "type": "module",
  "version": "0.1.1",
  "description": "Integration React Native Webview and Web",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gronxb/rnbridge.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "/dist/commonjs/index.cjs",
  "module": "/dist/module/index.mjs",
  "types": "/dist/typescript/packages/web/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module/index.mjs",
      "require": "./dist/commonjs/index.cjs",
      "types": "./dist/typescript/packages/web/src/index.d.ts"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "build": "node esbuild.config.js && tsc --emitDeclarationOnly",
    "test:type": "tsc --noEmit"
  },
  "devDependencies": {
    "esbuild": "^0.19.4"
  }
}
